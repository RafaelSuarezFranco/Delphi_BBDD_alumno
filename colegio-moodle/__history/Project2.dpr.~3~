program Project2;

uses
  Vcl.Forms,
  Unit2 in 'Unit2.pas' {Principal},
  Unit1 in 'Unit1.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TPrincipal, Principal);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
